                     </div>\n\n                            <div class=\"ms-fs-13\" ng-if=\"item.shipping_cost > 0\">\n                                {{'Item Shipping'|msTranslate}}:<span class=\"\">\n            {{item.shipping_cost|msCurrency:order.currency.name}}</span>\n                            </div>\n                            <div ng-if=\"item.taxes.length && ms.settings.show_item_level_taxes =='1'\">\n                                <p ng-if=\"item.prices_are != 'inclusive'\" class=\"cart-product-tax-name ms-mb-xs\">\n                                    <b ng-if=\"ms.variables.tax\">{{ms.variables.tax|truncate:15}}</b> <b\n                                        ng-if=\"!ms.variables.tax\">{{'Taxes'|msTranslate}}</b>:\n                                    <span class=\"price ms-fs-14 cart-product-tax-price\">\n                                    {{item.taxes_total|msCurrency:order.currency.name}}\n                                     </span>\n                                    ({{item.prices_are|ms_ucfirst}})\n                                </p>\n                                <p ng-if=\"item.prices_are == 'inclusive'\"\n                                   class=\"cart-product-tax-name ms-mb-xs\">\n                                    <b ng-if=\"ms.variables.tax\">{{ms.variables.tax|truncate:15}}</b> <b\n                                        ng-if=\"!ms.variables.tax\">{{'Taxes'|msTranslate}}</b>:\n                                    <span class=\"price ms-fs-14 cart-product-tax-price\">\n                                    {{item.taxes_total|msCurrency:order.currency.name}}\n                                </span>\n                                    ({{item.prices_are|ms_ucfirst}})\n                                </p>\n                            </div>\n\n                        </td>\n\n                        <td class=\"col-sm-4 ms-fs-13\">\n\n                            <span class=\"pull-right \"> {{item.price|msCurrency:order.currency.name}} <span ng-if=\"item.product.uom\">{{item.product.uom}} </span> </span>\n\n\n                        </td>\n                    </tr>\n                    <!--sub_total -->\n                    <tr>\n                        <td class=\"col-sm-4\"></td>\n\n\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5 ms-pt-s\">\n                            <span class=\"pull-right font-bold\">{{'Sub Total'|msTranslate}}: </span></td>\n\n\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3 ms-pt-s\">\n                            <span class=\"pull-right \">{{order.sub_total|msCurrency:order.currency.name}}</span></td>\n\n\n                    </tr>\n                    <!----shipping > discount > wallet > taxes(inclusive,exclusive) > additional charges-->\n                    <tr ng-if=\"order.shipping_total > 0\">\n                        <td class=\"col-sm-4\"></td>\n\n\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\">\n                            <span class=\"pull-right font-bold\">{{'Shipping charges'|msTranslate}}(+): </span></td>\n\n\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m  col-sm-3\">\n                            <span class=\"pull-right \">{{order.shipping_total|msCurrency:order.currency.name}}</span>\n                        </td>\n\n\n                    </tr>\n                    <tr>\n                        <td class=\"col-sm-4\"></td>\n                        <td ng-if=\"order.discounts\" ng-repeat=\"discount in order.discounts\"\n                            class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\"\n                        ><span class=\"pull-right font-bold\">  {{discount.name|lowercase|ms_ucfirst}}(-):</span>\n                        </td>\n                        <td ng-if=\"order.discounts\" ng-repeat=\"discount in order.discounts\"\n                            class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3\"\n                        ><span class=\"pull-right \">  {{discount.saved_amount|msCurrency:order.currency.name}}</span>\n                        </td>\n                    </tr>\n                    <tr>\n                        <td class=\"col-sm-4\"></td>\n                        <td ng-if=\"order.wallets.length\" ng-repeat=\"wallet in order.wallets\"\n                            class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\"\n                        ><span class=\"pull-right font-bold\"> {{wallet.wallet_name}}(-):</span>\n                        </td>\n\n                        <td ng-if=\"order.wallets.length\" ng-repeat=\"wallet in order.wallets\"\n                            class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3\"\n                        ><span class=\"pull-right \">{{wallet.amount|msCurrency:order.currency.name}}</span>\n                        </td>\n\n                    </tr>\n                    <tr ng-repeat=\"tax in order.taxes\"\n                        ng-if=\"tax.prices_are=='inclusive' && ms.settings.show_inclusive_taxes =='1'\">\n                        <td class=\"col-sm-4\"></td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\"><span\n                                class=\"pull-right font-bold\">{{tax.name}}(inclusive): </span>\n                        </td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3\">\n                            <span class=\"pull-right \">{{tax.tax_amount|msCurrency:order.currency.name}}</span>\n                        </td>\n                    </tr>\n                    <tr ng-repeat=\"tax in order.taxes\" ng-if=\"tax.prices_are!='inclusive'\">\n                        <td class=\"col-sm-4\"></td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\"><span\n                                class=\"pull-right font-bold\">{{tax.name}}(+): </span>\n                        </td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3\">\n                            <span class=\"pull-right \">{{tax.tax_amount|msCurrency:order.currency.name}}</span>\n                        </td>\n                    </tr>\n                    <tr ng-if=\"order.additional_charges\" ng-repeat=\"charge in order.additional_charges\">\n                        <td class=\"col-sm-4\"></td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-5\"><span\n                                class=\"pull-right font-bold\">{{charge.name}}(+):</span></td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m col-sm-3\"><span class=\"pull-right\">\n                {{charge.amount|msCurrency:order.currency.name}}</span>\n                        </td>\n\n                    </tr>\n                    <!--total-->\n                    <tr>\n                        <td class=\"col-sm-4\"></td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m ms-pb-s col-sm-5\"><b><span class=\"pull-right\">\n        {{'Grand Total'|msTranslate}}:</span\n                        ></b>\n                        </td>\n                        <td class=\"ms-lh-18 ms-fs-13 ms-pl-m ms-pb-s col-sm-3\"><span class=\"pull-right \">{{order.total|msCurrency:order.currency.name}}</span>\n                        </td>\n                    </tr>\n\n\n                    </tbody>\n                </table>\n            </div>\n            <a ng-hide=\"show_make_payment\" type=\"button\" id=\"continue_shop\" ng-href='/'\n               class=\"btn btn-primary pull-right ms-p-xs ms-mt-x ms-mb-s\">{{'Continue To Shop'|msTranslate}}</a>\n            <div class=\"ms-p-xs ms-mt-s ms-mb-s\" ng-if=\"show_make_payment\"></div>\n        </div>\n        <div class=\"\" ng-if=\"ms.screen.s || ms.screen.xs\">\n            <h1 class=\"ms-mt-s\">{{'Order Summary'|msTranslate}}</h1>\n            <div ng-repeat=\"order in order_array\">\n\n                <div class=\"ms-mt-s\" ng-if=\"show_make_payment && order.balance_amount\">\n                    <div class=\"alert alert-info ng-binding row\">\n          