ope.a = true;\n        $scope.b = true;\n        $scope.c = true;\n    }\n\n    $scope.submitting = false;\n    $scope.submit = function () {\n        if ($scope.submitting) {\n            return false;\n        }\n        var publish = '0'\n        if ($rootScope.ms.settings.auto_publish_reviews == '1') {\n            publish = '1'\n        }\n        if ($scope.review_text) {\n            if (!$scope.review_rating) {\n                alert('Ratings can not be blank');\n                return;\n            }\n            $scope.review_rating = $scope.review_rating ? $scope.review_rating : 0;\n            $scope.review = {\n                review: $scope.review_text,\n                publish: publish,\n                rating: $scope.review_rating,\n                product_name: $scope.product.name,\n                unique_id: $scope.settings.uniqueid,\n                user_id: $rootScope.ms.user._id,\n                email: $rootScope.ms.user.email,\n                error: ''\n            };\n            $scope.submitting = true;\n            entity.create({data: $scope.review}).then(function (data) {\n                $scope.a = true;\n                $scope.review_text = '';\n                $scope.review_rating = '';\n                $scope.rate = -1;\n                $rootScope.$broadcast('ReviewAdded');\n                $scope.$eval($attrs.msSuccess);\n                $scope.submitting = false;\n                $scope.submitted = false;\n            }, function () {\n                $rootScope.$emit('ReviewAdded');\n                $scope.submitting = false;\n            });\n\n        }\n        else {\n            $scope.submitted = true;\n            $scope.error = \"Review can not be blank\";\n        }\n    };\n}","settings":{}},"ms.form.search":{"name":"ms.form.search","type":"store","controller":"function ($scope, $stateParams, $element, $location, $rootScope) {\n    $scope.fields = {};\n    $scope.widgetName = \"ms.form.search\";\n    $scope.requiredfiel