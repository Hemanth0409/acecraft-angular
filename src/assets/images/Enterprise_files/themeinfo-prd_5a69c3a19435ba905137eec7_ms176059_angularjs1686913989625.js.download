ds.mode_of_payment = $scope.data.mode_of_payment;\n            }\n            if($scope.data.bank_name){\n                if(!returnOrderRequest.data.metafields) returnOrderRequest.data.metafields = {};\n                returnOrderRequest.data.metafields.bank_name = $scope.data.bank_name;\n            }\n            if($scope.data.branch_name){\n                if(!returnOrderRequest.data.metafields) returnOrderRequest.data.metafields = {};\n                returnOrderRequest.data.metafields.branch_name = $scope.data.branch_name;\n            }\n            if($scope.data.ifsc_code){\n                if(!returnOrderRequest.data.metafields) returnOrderRequest.data.metafields = {};\n                returnOrderRequest.data.metafields.ifsc_code = $scope.data.ifsc_code;\n            }\n            if($scope.data.account_number){\n                if(!returnOrderRequest.data.metafields) returnOrderRequest.data.metafields = {};\n                returnOrderRequest.data.metafields.account_number = $scope.data.account_number;\n            }\n            if($scope.data.customer_name){\n                if(!returnOrderRequest.data.metafields) returnOrderRequest.data.metafields = {};\n                returnOrderRequest.data.metafields.customer_name = $scope.data.customer_name;\n            }\n            console.log('returnOrder====00',returnOrderRequest)\n            app.call('returnOrder', returnOrderRequest, function (err, response) {\n                if (err || !response || !response.data) {\n                    return alert('Error while placing the return request. Please try again later.');\n                }\n                $scope.returnData.selected_items = [];\n                order.return_submitted = true;\n                alert('Return Request submitted successfully!');\n                $scope.data = {};\n                $scope.getUserOrders();\n                if ($('#myReturnModal')) {\n                    $('#myReturnModal').modal('hide');\n                }\n       