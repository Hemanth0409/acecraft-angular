function () {\n        $scope.counter = $scope.counter - 1 ;\n\n        if($scope.counter === 0){\n            $interval.cancel(interval);\n        }\n\n    }, 1000);\n\n\n}","settings":{}},"ms.rvpointsCtrl":{"name":"ms.rvpointsCtrl","type":"store","controller":"function ($scope, $rootScope, app) {\n    console.log('>>>>> here in controller rvpoints');\n    var calculate_rv = function () {\n        var products = $rootScope.ms.cart.items;\n        var total = 0;\n        angular.forEach(products, function (item) {\n            if (item.product.metafields && item.product.metafields.rv) {\n                total += item.product.metafields.rv * item.quantity;\n            }\n        });\n\n        return total;\n    }\n\n    $scope.total_rv_points = calculate_rv();\n    $rootScope.ms.cart.total_rv_points = calculate_rv();\n\n    $rootScope.$watch('ms.cart.item_count', function (newValue, oldValue) {\n        console.log('watchh', newValue, oldValue)\n        if (newValue == oldValue)return;\n\n        $scope.total_rv_points = calculate_rv();\n        $rootScope.ms.cart.total_rv_points = calculate_rv();\n\n    }, true);\n\n}","settings":{}},"ms.th2.poweredfooter":{"name":"ms.th2.poweredfooter","type":"store","settings":{}},"ms.forms":{"name":"ms.forms","type":"store","controller":"mystore.controllers.forms","settings":[{"name":"formname"}]},"ms.th3.form_layout1":{"name":"ms.th3.form_layout1","type":"store"},"ms.seller":{"name":"ms.seller","type":"store","controller":"function ($scope, $rootScope, app, $location) {\n    $scope.user = {};\n    //forgot password and reset password functionality\n    $scope.showLogin = true;\n    $scope.showForgot = false;\n    $scope.showReset = false;\n    $scope.showVerification = false;\n    $scope.errors = {};\n\n    //going to get store information\\\n    $scope.login_loading = true;\n    if (!$rootScope.ms) {\n        $rootScope.ms = {};\n    }\n    $scope.store = $rootScope.ms.store;\n    $scope.store = {};\n\n    var userVerificatio