g\">\n    <div class=\"col-sm-6 ms-mt-m ms-mb-m\">\n        <div ng-if=\"ms.settings.copyright\" class=\"pull-left copyright ms-fs-13\">\n            {{'Copyright'|msTranslate}} © <a href=\"/\" title=\"{{ms.settings.store_title}}\">{{ms.settings.copyright}}</a>\n        </div>\n    </div>\n    <div class=\"col-sm-6 ms-mt-m ms-mb-m\">\n        <div class=\"pull-right powered-logo ms-fs-14\">\n            <b><a class=\"ecommerce\" target=\"_blank\" rel=\"noopener\" href=\"https://www.storehippo.com\" title=\"Powered by StoreHippo\">{{'Powered\n                by\n                StoreHippo' |msTranslate}}</a></b>\n        </div>\n    </div>\n</div>","/templates/widgets/ms.th3.form_layout1.html":"<div class=\"row\">\n    <div class=\"col-sm-12 ms-mt-m\">\n        <div ng-repeat=\"field in fields\">\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='captcha'\">\n                <label for=\"{{field.name}}\">{{'Captcha'|msTranslate}}</label>\n                <div ms-widget=\"ms.form.captcha\" ms-data-src=\"cdn\"></div>\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='email'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n\n                <input type=\"text\" ng-readonly=\"field.settings.read_only\" class=\"form-control\" id=\"{{field.name}}\"\n                       ng-model='formData[field.name]'\n                       placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n                <label class=\"error\">{{errors[field.name]}}</label>\n\n            </div>\n            <div class=\"form-group ms-mb-xs\" data-ng-if=\"field.settings.display_type=='phone'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <input type=\"tel\" id=\"phone\" class=\"form-control\" data-ng-model=\"formData[field.name]\"\n                       data-ng-intl-tel-input placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='text'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n\n                <input type=\"text\" ng-readonly=\"field.settings.read_only\" class=\"form-control\" id=\"{{field.name}}\"\n                       ng-model='formData[field.name]'\n                       placeholder=\"{{field.settings.placeholder|msTranslate}}\" autocomplete=\"off\">\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='password'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <input type=\"password\" class=\"form-control\" id=\"{{field.name}}\" autocomplete=\"off\"\n                       placeholder=\"{{field.settings.placeholder|msTranslate}}\" ng-model='formData[field.name]'>\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"checkbox\" ng-if=\"field.settings.display_type=='checkbox'\">\n                <label>\n                    {{field.label|msTranslate}}\n                    <input type=\"checkbox\"  ng-true-value=\"1\"\n                           ng-false-value=\"0\" ng-model='formData[field.name]'>\n                </label>\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='file'\" ms-widget=\"ms.instantFileUpload\"\n                 ms-data-verisioning=\"{{field.settings.disable_versioning}}\" ms-data-name=\"file_option\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n\n                <input id=\"{{uniqueId}}\" type=\"file\" class=\"form-control\" ng-model=\"formData[field.name]\" data-name=\"file_option\"\n                       placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n\n                <div ng-if=\"uploading\">\n                    <i class=\"fa fa-cog text-center fa-spin fa-lg\"></i> Loading...\n                </div>\n                <div ng-if=\"!uploading\">\n                    <div ng-if=\"previewUrl\">\n                        <img ng-src=\"{{previewUrl}}\" style=\"height:100px;\"/>\n                    </div>\n                </div>\n                <label class=\"error\">{{errors[field.name]}}</label>\n\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='select'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n\n                <select id=\"{{field.name}}\" class=\"form-control\" ng-model='formData[field.name]'\n                        ng-options=\"option.value as option.name for option in field.settings.options \">\n\n                </select>\n                <label class=\"error\">{{errors[field.name]}}</label>\n\n            </div>\n            <div class=\"form-group ms-mb-0\" ng-if=\"field.settings.display_type=='date'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <input class=\"form-control\" ui-date-format=\"DD, d MM, yy\" ui-date ng-model='formData[field.name]'\n                       placeholder=\"{{field.settings.placeholder}}\">\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"form-group ms-mb-xs\" ng-if=\"field.settings.display_type=='widget'\">\n                <div ms-widget=\"{{field.settings.widget}}\"></div>\n            </div>\n\n            <div class=\"form-group ms-mb-xs\" ng-if=\"field.settings.display_type=='state'\">\n                <div ms-widget=\"ms.statePicker\" ms-data-country-field='formData.country'>\n                    <div ng-if='states.length!=0' class=\"form-group\">\n                        <label for=\"country\"><b>{{field.label|msTranslate}}</b></label>\n                        <select name=\"state\" class=\"form-control\" id=\"state\" placeholder=\"{{'State'|msTranslate}}\"\n                                ng-model=\"formData[field.name]\"\n                                ng-options=\"data.name as data.name for data in states\">\n                        </select>\n                        <span style=\"color:#F30B39\" class=\"error\"\n                              ng-show=\"errors.state\">{{errors.state}}</span>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"form-group ms-mb-xs\" ng-if=\"field.settings.display_type=='country'\">\n                <div ms-widget=\"ms.countryPicker\">\n                    <div ng-if=\"countries.length==1\">\n                        <div for=\"country\" ng-init=\"fields[field.name]=countries[0].code\">\n                                                  <span class=\"text-muted\">{{'Service available\n                                only in'|msTranslate}} {{countries[0].country}}\n                                                  </span>\n                        </div>\n                    </div>\n                    <div ng-if=\"countries.length>1\">\n                        <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                        <select name=\"country\" class=\"form-control\" id=\"{{field.name}}\"\n                                ng-model=\"formData[field.name]\"\n                                ng-options=\"data.code as data.country for data in countries\"  placeholder=\"{{'Country'|msTranslate}}\">\n                            <option value=''>{{'Select'|msTranslate}} {{field.name|msTranslate}}</option>\n                        </select>\n                        <label class=\"error\">{{errors[field.name]}}</label>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='radio'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <label class=\"radio-inline\" data-ng-repeat=\"option in field.settings.options\">\n                    <input type=\"radio\" ng-model='formData[field.name]' name=\"{{field.name}}\" id=\"{{field.name}}\"\n                           ng-value=\"option.value\"> {{option.name}}\n                </label>\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n            <div class=\"form-group\" ng-if=\"field.settings.display_type=='textarea'\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <textarea id=\"{{field.name}}\" class=\"form-control\" ng-model='formData[field.name]' rows=\"3\"\n                          placeholder=\"{{field.settings.placeholder}}\"></textarea>\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n            <div class=\"form-group ms-mb-xs\" ng-if=\"field.settings.display_type=='multiselect'\" style=\"display: block;\">\n                <label for=\"{{field.name}}\">{{field.label|msTranslate}}</label>\n                <div ms-widget=\"ms.form.multiselect2\" style=\" padding: 5px 0px;\"></div>\n                <label class=\"error\">{{errors[field.name]}}</label>\n            </div>\n\n        </div>\n\n    </div>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-sm-12 form-group\">\n        <div class=\"text-center\">\n            <button type=\"submit\" class=\"btn btn-primary form-button\">{{button|ms_ucfirst|remove_underscore|msTranslate}}</button>\n        </div>\n    </div>\n</div>","/templates/widgets/ms.seller.html":"<span ng-if=\"ms.routeParams.action !='login'\">\n    {{ms.location.path('/seller/login')}}\n</span>\n\n<section>\n    <div class=\"container\">\n        <div class=\"wrapper ms-mt-35x ms-mb-35x ms-pt-x ms-pb-x\">\n      