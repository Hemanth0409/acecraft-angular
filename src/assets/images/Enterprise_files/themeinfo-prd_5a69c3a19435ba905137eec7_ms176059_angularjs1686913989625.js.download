      </tr>\n\n                    <tr ng-if=\"(order.payment_method.type=='offline'|| order.payment_method.type=='cash_on_delivery') && $index === 0\">\n                        <th>{{'Payment Method'|msTranslate}}</th>\n                        <td>{{order.payment_method.name|lowercase|ms_ucfirst}}\n                        </td>\n                    </tr>\n\n                    <tr ng-if=\"$index === 0 && order.shipping_method.name\">\n                        <th>{{'Shipping Method'|msTranslate}}</th>\n                        <td>{{order.shipping_method.name|lowercase|ms_ucfirst}}</td>\n                    </tr>\n\n                    <tr ng-if=\"$index === 0\">\n                        <th colspan=\"2\">{{'Shipping Address'|msTranslate}}</th>\n                    </tr>\n                    <tr ng-if=\"$index === 0\">\n                        <td colspan=\"2\">{{order.shipping_address.full_name|lowercase|ms_ucfirst}}<br>\n                            {{order.shipping_address.address}}, {{order.shipping_address.city}},\n                            {{order.shipping_address.state}}<br>\n                            {{order.shipping_address.country}}-{{order.shipping_address.zip}}<br>\n                            {{order.shipping_address.phone}}\n                        </td>\n                    </tr>\n\n                    <tr>\n                        <th>{{'Order ID'|msTranslate}}</th>\n                        <th>{{order.order_id}}</th>\n                    </tr>\n\n                    <tr>\n                        <th>{{'Sold By'|msTranslate}}</th>\n                        <th>{{order.seller_name}}</th>\n                    </tr>\n\n\n                    <tr>\n                        <td colspan=\"2\">\n\n                            <table class=\"table table-bordered table-striped\">\n                                <thead>\n                                <tr>\n                                    <th colspan=\"2\">{{'Item'|msTranslate}}</th>\n                     