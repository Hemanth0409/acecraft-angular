          {{ms.variables.sub_total|truncate:15}}<b>:&nbsp;<span\n                                    class=\"price\"\n                                    ng-bind=\"ms.cart.sub_total|msCurrency\"></span> </b></td>\n                            <td class=\n                                        \"pull-right\" ng-if=\"!ms.variables.sub_total\">{{'Sub Total'|msTranslate}}<b>:&nbsp;<span\n                                    class=\"price\"\n                                    ng-bind=\"ms.cart.sub_total|msCurrency\"></span> </b></td>\n                        </tr>\n\n                        <tr ng-if=\n                                    \"ms.cart.shipping_method\">\n                            <td class=\n                                        \"pull-right\">\n                                <small>{{ms.cart.shipping_method.name}}:</small>\n                                <b><span>{{ms.cart.shipping_total|msCurrency}}</span></b></td>\n                        </tr>\n                        <tr ng-if=\"ms.cart.wallets.length\">\n                            <td class=\n                                        \"pull-right\" ng-repeat=\"wallet in ms.cart.wallets\">\n                                <b>{{wallet.wallet_name}} :</b>\n                                <small>{{wallet.amount}}</small>\n                            </td>\n                        </tr>\n                        <tr ng-if=\n                                    \"ms.cart.discounts_total>0\">\n                            <td class=\n                                        \"pull-right\"><span ng-if=\"ms.variables.order_discount\">{{ms.variables.order_discount|truncate:15}}</span> <span\n                                    ng-if=\"!ms.variables.order_discount\">{{'Discount'|msTranslate}}</span> <span\n                                    ng-repeat=\"discount in\n        ms.cart.discounts\"> <span>{{discount.name}}:</span>&nbsp; <b><span>{{discount.saved_amount|msCurrency}}</span></b>                        </span>\n                            </td>\n                        </tr>\n                        <tr ng-if=\n                                    \"ms.cart.taxes_total>0\">\n                            <td class=\n                                        \"pull-right\">                <!---------------------------------->\n                                <div class=\n                                             \"panel-group\" id=\"accordion1\" role=\"tablist\" aria-multiselectable=\"true\">\n                                    <div class=\n                                                 \"panel\" style=\"box-shadow: 0px 0px\">\n                                        <div class=\n                                                     \"\" role=\"tab\" id=\"headingOne1\" data-ng-init=\"pos1='up'\"><a\n                                                data-toggle=\"collapse\"\n                                                data-ng-click=\"pos1='down'\" data-ng-show=\"pos1=='up'\"\n                                                data-parent=\"#accordion1\"\n                                                href=\"#collapseOne1\"\n                                                aria-expanded=\"true\" aria-controls=\"collapseOne1\"> <span\n                                                ng-if=\"ms.cart.prices_are=='exclusive' \"><i class=\"fa\n        fa-chevron-right\"></i>&nbsp;<span\n                                                ng-if=\"ms.variables.tax_exclusive\">{{ms.variables.tax_exclusive|truncate:15}}:</span> <span\n                                                ng-if=\"!ms.variables.tax_exclusive\">{{'Tax\n        Collected:'|msTranslate}}</span></span> <span ng-if=\"ms.cart.prices_are!='exclusive' \"><i class=\"fa\n        fa-chevron-right\"></i>&nbsp;<span\n                                                ng-if=\"ms.variables.tax_inclusive\">{{ms.variables.tax_inclusive|truncate:15}}:</span> <span\n                                                ng-if=\"!ms.variables.tax_inclusive\">{{'Tax\n        Included:'|msTranslate}}</span></span>\n                                            <b><span>{{ms.cart.taxes_total|msCurrency}}</span></b> </a> <a\n                                                data-toggle=\"collapse\"\n                                                data-ng-click=\"pos1='up'\"\n                                                data-ng-show=\"pos1=='down'\"\n                                                data-parent=\"#accordion1\"\n                                                href=\"#collapseOne1\"\n                                                aria-expanded=\"true\"\n                                                aria-controls=\"collapseOne1\"> <span\n                                                ng-if=\"ms.cart.prices_are=='exclusive' \"><i class=\"fa\n        fa-chevron-down\"></i>&nbsp;<span\n                                                ng-if=\"ms.variables.tax_exclusive\">{{ms.variables.tax_exclusive|truncate:15}}:</span> <span\n                                                ng-if=\"!ms.variables.tax_exclusive\">{{'Tax\n        Collected:'|msTranslate}}</span></span> <span ng-if=\"ms.cart.prices_are!='exclusive' \"><i class=\"fa\n        fa-chevron-down\"></i>&nbsp;<span\n                                                ng-if=\"ms.variables.tax_exclusive\">{{ms.variables.tax_exclusive|truncate:15}}:</span> <span\n                                                ng-if=\"!ms.variables.tax_exclusive\">{{'Tax\n        Included:'|msTranslate}}</span></span>\n                                            <b><span>{{ms.cart.taxes_total|msCurrency}}</span></b> </a></div>\n\n                                        <div id=\n                                                     \"collapseOne1\" class=\"panel-collapse collapse \" role=\"tabpanel\"\n                                             aria-labelledby=\"headingOne1\">\n                                            <div class=\"panel-body ms-pr-0 gray-light pull-right\"\n                                                 style=\"padding:10px;background:#fff!important\">\n                                                <div data-ng-repeat=\n                                                             \"tax in ms.cart.taxes\">\n                                                    <span>{{tax.name}}</span>:&nbsp;<span>{{tax.tax_amount|msCurrency}}</span>\n                                                </div>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                                <!--------------------------------------->            </td>\n                        </tr>\n                        <tr ng-if=\n                                    \"ms.cart.additional_charges.length\">\n                            <td class=\n                                        \"pull-right\"><span ng-if=\"ms.variables.additional_charges\">{{ms.variables.additional_charges|truncate:20}}:</span>\n                                <span ng-if=\"!ms.variables.additional_charges\">{{'Additional Charges:'|msTranslate}} </span> <span\n                                        ng-repeat=\"charge\n        in ms.cart.additional_charges\"> <span>{{charge.name}}</span>\n        <span>{{charge.amount|msCurrency}}</span>                        </span></td>\n                        </tr>\n                        <tr ng-if=\"ms.variables.show_total_cashback=='yes' || ms.variables.show_total_cashback=='Yes' || ms.variables.show_total_cashback=='YES'\">\n                                      <span ms-widget=\"ms.rvpointsCtrl\">\n                                    <td class=\"pull-right\" ng-if=\"ms.cart.total_rv_points\"><b>{{'Total\n                                        CashBack:'|msTranslate}}&nbsp;</b>{{ms.cart.total_rv_points}}\n                                    </td></span>\n                        </tr>\n                        <tr>\n\n                            <td class=\n                                        \"pull-right\" ng-if=\"ms.variables.total\">{{ms.variables.total|truncate:15}}<b>:&nbsp;<span\n                                    class=\"price\"\n                                    ng-bind=\"ms.cart.total|msCurrency\"></span></b></td>\n                            <td class=\n                                        \"pull-right\" ng-if=\"!ms.variables.total\">{{'Total'|msTranslate}}<b>:&nbsp;<span\n                                    class=\"price\"\n                                    ng-bind=\"ms.cart.total|msCurrency\"></span></b></td>\n                        </tr>\n\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n            <div class=\"row\">\n                <div class=\"col-sm-12 ms-mt-m\">\n                    <div ng-if=\"ms.cart.payment_method.type=='offline'||ms.cart.payment_method.type=='cash_on_delivery'\"\n                         class=\"ms-mb-m pull-right\">\n                        <button type=\"submit\" ng-click='create_order()'\n                                class=\"btn-lg ms-pl-x ms-pr-x ms-fs-18 btn btn-primary\">\n                            {{'Confirm'|msTranslate}}\n                        </button>\n                    </div>\n                    <div ng-if='ms.cart.payment_method.type==\"payment_gateways\"' class=\"ms-mb-m text-right\">\n                        <button type=\"submit\" ng-click='create_order()'\n                                class=\"btn ms-pl-x ms-pr-x ms-fs-18 btn-primary btn-lg\">\n                            {{'Pay Now'|msTranslate}}\n                        </button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<style>    .table-striped > tbody > tr:nth-child(odd) > td {\n    background-color: #fff;\n\n}\n\n.table-striped > thead > tr > th {\n    background-color: #f1f1f1;\n\n}\n\n.checkout-button {\n    display: none;\n}\n\n.billing-address-button {\n    display: none;\n}\n\n.error {\n    color: red;\n}\n\n</style>\n","/templates/widgets/ms.seller_info_new.html":"<div ng-if=\"!loading\">\n    <div class=\"ms-mt-xs ms-mb-s\" ng-if=\"seller.title\">\n        <b>{{'Sold By'|msTranslate}}: </b>\n        <a href=\"/vendor/{{seller._id}}\">{{seller.title}}</a>\n    </div>\n    <div ng-if=\"seller._id\" class=\"inline-block \" ms-widget=\"ms.loadvendorreview\" ms-data-unique_id=\"{{seller._id}}\"\n         ms-name=\"loadvendorreview\">\n        <div ng-if=\"reviews.avg_rating>0\" class=\"rating-box ms-mb-x\" id=\"input3\" ng-init=\"r=reviews.avg_rating\">\n            <b>{{'Seller Rating'|msTranslate}}: </b>\n            <i class=\"fa fa-star fa\" id=\"rating\" data-ng-class=\"{active: $index < reviews.avg_rating}\"\n               data-ng-repeat=\"star in [0,1,2,3,4]\">\n            </i>\n        </div>\n    </div>\n</div>\n","/templates/widgets/ms.th3.getFacets.html":"","/templates/widgets/ms.socialShare.html":"<div id=\"share\">\n    <a ng-click=\"shareAll()\" class=\"ms-mb-xs btn btn-sm btn-primary add-cart ms-fs-16\"\n       title=\"{{'Share'|msTranslate}}\">\n      <i class=\"fa fa-share-alt\"></i> Share\n    </a>\n</div>","/templates/widgets/ms.form.userVerification.html":"<div class=\"reset-password-box user-box\">\n    <h2 class=\"text-center secondary ms-mb-x  main-heading\">\n        {{'Verification'|msTranslate}}</h2>\n\n    <form class=\"form-layout-new ms-mt-x ms-pt-x\" name=\"verifyUser\">\n        <div ng-if=\"ms.user.verified !='1' && (!ms.settings.verification_method || ms.settings.verification_method == 'email' || ms.settings.verification_method == 'both')\">\n            <h4 class=\"text-center secondary ms-mb-x  main-heading\">\n                {{'Email Verification'|msTranslate}}</h4>\n\n            <div class=\"form-group\">\n                <button class=\"btn btn-primary ms-fs-18 ms-p-s  btn-block\"\n                        