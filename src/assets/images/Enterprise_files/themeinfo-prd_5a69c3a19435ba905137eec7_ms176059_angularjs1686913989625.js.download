abel\": ms.substore && ms.substore.name ? ms.substore.name : ms.store.name,\n                \"type\": \"final\",\n                \"amount\": parseFloat(ms.cart.total).toFixed(ms.cart.currency.decimal_points)\n            }\n        };\n        console.log(\"Starting apple pay session\", paymentRequest);\n        if(typeof ApplePaySession == 'undefined') return;\n        var applePaySession = new ApplePaySession(1, paymentRequest);\n        $scope.create_order_applepay(applePaySession);\n    }\n\n\n    $scope.$watch('ms.user', function (newValue, oldValue) {\n        if (newValue == oldValue) return;\n        if (ms.user && ms.user.isLoggedIn) {\n            $scope.step = 2;\n        }\n        else {\n            $scope.step = 1;\n        }\n    });\n    var step = 1;\n    if (!cart.valid_email_id) {\n        step = 1;\n    }\n    else {\n        step = 1;\n    }\n    $scope.goto_step(step);\n\n}","settings":{}},"ms.shippingAddress_controller":{"name":"ms.shippingAddress_controller","type":"store","controller":"function ($scope, $rootScope, $location, $element, $attrs, app) {\n\n    window.mystore.formValidator($scope, app);\n\n    if (Object.keys($rootScope.ms.cart.shipping_address).length) {\n        $scope.fields = angular.copy($rootScope.ms.cart.shipping_address);\n    }\n\n    $rootScope.fill_shipping_address = function (address) {\n        $scope.fields = angular.copy(address);\n    }\n\n    $scope.submit = function () {\n\n        if (!$scope.submitValid()) {\n\n            return;\n        }\n        $scope.submitting = true;\n        $scope.submitting = false;\n        $scope.fields = {};\n        $scope.$parent.$parent.$eval($attrs.msSuccess);\n    }\n}","settings":{}},"ms.billingAddress_controller":{"name":"ms.billingAddress_controller","type":"store","controller":"function ($scope, $rootScope, $location, $element, $attrs, app) {\n\n    window.mystore.formValidator($scope, app);\n\n    if (Object.keys($rootScope.ms.cart.billing_address).length) {\n     