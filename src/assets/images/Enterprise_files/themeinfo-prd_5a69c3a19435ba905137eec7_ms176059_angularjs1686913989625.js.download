  {{'Forgot'|msTranslate}}?\n                                        &nbsp;</a>\n                                    <div data-ng-if=\"errors.password\" class=\"error\">{{errors.password}}</div>\n                                </div>\n                                <div ng-if=\"roles\" class=\"form-group ms-mb-xs\">\n                                    <label for=\"role\">{{'Select a role'|msTranslate}}</label>\n                                    <select id=\"role\" data-ng-model=\"user.role\">\n                                        <option data-ng-value=\"opt\" data-ng-repeat=\"opt in roles\">{{ opt }}</option>\n                                    </select>\n                                    <div data-ng-if=\"errors.role\" class=\"error\">{{errors.role}}</div>\n                                </div>\n\n                                <button type=\"submit\" class=\"submit-button btn-primary\"\n                                        data-ng-disabled=\"login.$invalid\"\n                                        data-ng-submit=\"adminLogin()\">\n                                    <b style=\"color: #ffffff!important\">{{'LOG IN'|msTranslate}}</b>\n                                </button>\n                                <div class=\"ms-mt-x ms-mp-x text-center\">\n                                    <p>{{'New Here'|msTranslate}} ? <span>\n                                        <a ng-click=\"ms.location.path('/sellers/register')\">\n                                            {{'Register Here'|msTranslate}}\n                                        </a></span>\n                                    </p>\n                                </div>\n                            </form>\n\n                            <form data-ng-if=\"showForgot\" name=\"forgotpassword\">\n                                <p style=\"margin-bottom: 30px;\" class=\"title text-center\">\n                                    {{'Recover your password'|msTranslate}}\n                                </p>\n\n                                <div class=\"form-group ms-mb-xs\">\n                                    <label for=\"femail\">{{'Email'|msTranslate}}</label>\n                                    <input type=\"email\" class=\"form-control\" id=\"femail\" data-ng-model='user.email'\n                                           placeholder=\"Email\">\n                                    <div data-ng-if=\"errors.email\" class=\"error\">{{errors.email}}</div>\n                                </div>\n                                <button type=\"submit\" class=\"submit-button btn-primary\"\n                                        data-ng-disabled=\"forgotpassword.$invalid\"\n                                        data-ng-click=\"forgotPassword()\">\n                                    <span style=\"color: #ffffff!important\">\n                                        {{'SEND RESET LINK'|msTranslate}}\n                                    </span>\n                                </button>\n                                <div class=\"text-center\">\n                                    <a class=\"btn-primary forgot-password\" href=\"\" data-ng-click=\"showLoginForm()\">\n                                        <b>{{'Go back to login'|msTranslate}}</b>\n                                    </a>\n                                </div>\n                            </form>\n\n                            <form data-ng-if=\"showReset\" name=\"reset-password\" data-ng-submit=\"resetPassword()\">\n                                <p style=\"margin-bottom: 30px;\" class=\"title text-center\">\n                                    {{'Enter Your New Password'|msTranslate}}\n                                </p>\n                                <div class=\"form-group ms-mb-xs\">\n                                    <label for=\"rpassword\">{{'Password'|msTranslate}}</label>\n                                    <input type=\"password\" class=\"form-control\" id=\"rpassword\"\n                                           data-ng-model='user.password'\n                                           placeholder=\"Password\"/>\n                                    <div data-ng-if=\"errors.password\" class=\"error\">{{errors.password}}</div>\n                                </div>\n                                <div class=\"form-group ms-mb-xs\">\n                                    <label for=\"cpassword\">{{'Confirm Password'|msTranslate}}</label>\n                                    <input type=\"password\" class=\"form-control\" id=\"cpassword\"\n                                           data-ng-model='user.confirm_password'\n                                           placeholder=\"{{'Confirm Password'|msTranslate}}\"/>\n                                    <div data-ng-if=\"errors.confirm_password\" class=\"error\">\n                                        {{errors.confirm_password}}\n                                    </div>\n                                </div>\n\n                                <button type=\"submit\" class=\"submit-button btn-primary \"\n                                        style=\"margin-top: 5px!important\" data-ng-submit=\"resetPassword()\">\n                                    <b style=\"color: #ffffff!important\">{{'RESET'|msTranslate}}</b>\n                                </button>\n\n                            </form>\n\n                            <form data-ng-if=\"showVerification\" name=\"verification\">\n                                <p style=\"margin-bottom: 30px;\" class=\"title text-center\">{{'Verification'|msTranslate}}</p>\n                                <div data-ng-if=\"user.verified !='1' && (!ms.settings.seller_verification_method || ms.settings.seller_verification_method == 'email' || ms.settings.seller_verification_method == 'both')\">\n\n                                    <p class=\"text-center\">{{'Email Verification'|msTranslate}}</p>\n\n                                    <button class=\"submit-button btn-primary\"\n                                            data-ng-click=\"sendVerificationMail()\">\n                                        <span style=\"color: #ffffff!important\">{{'Resend Verification Email'|msTranslate}}</span>\n                                    </button>\n                                    <hr/>\n                                </div>\n\n                                <div data-ng-if=\"user.phone_verified !='1' && (ms.settings.seller_verification_method == 'phone' || ms.settings.seller_verification_method == 'both')\">\n                                    <p class=\"text-center\">{{'Mobile Verification'|msTranslate}}</p>\n                                    <div class=\"form-group ms-mb-xs\">\n                                        <label for=\"otp\">{{'OTP'|msTranslate}}</label>\n                                        <input type=\"text\" class=\"form-control\" id=\"otp\" data-ng-model='user.otp'\n                                               placeholder=\"{{'OTP'|msTranslate}}\">\n                                        <a data-ng-click=\"sendOtp()\"\n                                           style=\"position: absolute;right: 0;line-height: 19px;top: 40%;font-size: 13px;color: #7B7B7B;cursor: pointer\">\n                                            {{'Resend OTP'|msTranslate}}?\n                                        </a>\n                                        <div data-ng-if=\"errors.otp\" class=\"error\">{{errors.otp}}</div>\n                                    </div>\n\n                                    <button class=\"submit-button btn-primary\"\n                                            data-ng-click=\"verifyOtp()\">\n                                        <span style=\"color: #ffffff!important\">{{'Submit OTP'|msTranslate}}</span>\n                                    </button>\n\n                                </div>\n                                <div class=\"text-center\">\n                                    <a class=\"md-primary forgot-password\" href=\"\" data-ng-click=\"showLoginForm()\">\n                                        <b>{{'Go back to login'|msTranslate}}</b>\n                                    </a>\n                                </div>\n                            </form>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>\n<style>\n    .form-group {\n        position: relative;\n    }\n\n    #login-v2-form .separator {\n        font-weight: 600;\n        margin: 24px auto 16px;\n        position: relative;\n        overflow: hidden;\n        width: 100px;\n        text-align: center;\n        color: rgba(0, 0, 0, .54);\n    }\n\n    #login-v2-form .separator .text:after, #login-v2-form .separator .text:before {\n        content: '';\n        display: block;\n        width: 30px;\n        position: absolute;\n        top: 10px;\n        border-top: 1px solid rgba(0, 0, 0, .12);\n    }\n\n    #login-v2-form .separator .text:after {\n        left: 70%;\n    }\n\n    :after, :before {\n        box-sizing: border-box;\n    }\n\n    .text-center {\n        text-align: center;\n    }\n\n    .submit-button {\n        width: 100%;\n        margin: 16px auto;\n        display: block;\n        padding: 5px 16px;\n        font-size: 15px;\n        line-height: 1.33;\n        border-radius: 6px;\n    }\n\n    .forgot-password {\n        font-size: 13px;\n        font-weight: 500;\n        margin-bottom: 16px;\n    }\n\n    #login-v2-form {\n        padding: 0px 33px 48px;\n    }\n\n    #login-v2-form .title {\n        font-size: 21px;\n    }\n\n    #login-v2-form form {\n        width: 100%;\n        text-align: left;\n        padding-top: 14px;\n    }\n\n    #login-form {\n        width: 384px;\n        max-width: 384px;\n        min-width: 300px;\n        background: #FFF;\n        height: 100%;\n    }\n\n    .element {\n        position: relative;\n        top: 50%;\n        transform: translateY(-50%);\n    }\n\n    .user-box {\n        padding: 20px;\n        background-color: #fff;\n        box-shadow: 0 0 15px #d3d3d3;\n        min-height: 510px;\n    }\n\n</style>","/templates/widgets/password.html":"<style>@media screen and (max-width:320px){.abc{margin-top: 5px;important} .top-padding{margin-top: 30px!important}.password-btn{ margin-top: 5px!important; }.outer{height: 120vh!important; }}</style><div> <div class=\"custom_css\" ng-init=\"a=1\" style=\"position: fixed;top: 0;bottom: 0;width: 100%;z-index: 999999999999999;overflow: hidden;min-height: 100vh;background-image: url('https://cdn.storehippo.com/s/55fe862b8bfcc38f394402bb/ms.files/storehippo_password.png');\"> <div class=\"outer\"><a class=\"btn pull-right\" ng-if=\"p_flag\" ng-click=\"password_fun()\" style=\"color: #7796A8; position: fixed; top: 5px; right: 5px\"> <i class=\"fa fa-lock\" aria-hidden=\"true\"></i> Enter using Password </a> <a class=\"btn pull-right\" ng-if=\"!p_flag\" ng-click=\"password_fun()\" style=\"color: #7796A8; position: fixed; top: 5px; right: 5px\"> <i class=\"fa fa-times\" aria-hidden=\"true\"></i></a> <div class=\"middle\"> <div class=\"inner\"> <div class=\"container\"> <div class=\"row\"> <div class=\"col-sm-3\"></div><div class=\"col-sm-6 top-padding\" style=\"text-align: center;\" ng-if=\"p_flag\"><img ms-data-valign=\"middle\" class=\"ms-mh-90 ms-dis-ib img-responsive\" data-ng-src=\"{{ms.settings.logo|image:'480x480'}}\" title=\"{{ms.settings.store_title}}\" alt=\"{{ms.settings.store_title}}\"> <h1><b>Launching Soon </b></h1> <div class=\"ms-pb-l\" style=\"font-size: 19px;padding-top: 46px;\"><b>Be the first to know when we launch.</b></div><form><input ng-if=\"!msg\" class=\"ms-p-xs ms-pl-s\" id=\"emailAddress\" type=\"email\" placeholder=\"Email address\" style=\"width: 230px\" ng-model=\"notify.email\"></input> <button ng-if=\"!msg\" ng-disabled=\"!notify.email\" type=\"submit\" name=\"commit\" class=\"btn ms-pl-l abc\