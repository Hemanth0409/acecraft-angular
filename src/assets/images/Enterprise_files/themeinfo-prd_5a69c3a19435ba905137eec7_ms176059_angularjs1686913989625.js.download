nsole.log('$rootScope.commentId ==== ', $rootScope.commentId);\n        console.log('$scope.settings.uniqueid====', $scope.settings.uniqueid);\n        console.log($scope, '==$scope.settings.commentid====');\n        entity.list({\n            q: {\"_id\": $rootScope.commentId, \"publish\": \"1\", \"total\": \"1\"},\n            sort: '-created_on'\n        }).then(function (data) {\n//console.log($scope.comments,'====data====', data)\n//$scope.comments[0].reply = data.records[0].reply;\n            angular.forEach($scope.comments, function (comment) {\n                if (comment._id == $rootScope.commentId) {\n                    comment.reply = data.records[0].reply;\n                }\n            })\n//console.log('$scope.comments ===22=== ', $scope.comments);\n//$scope.comments = data.records;\n        });\n    };\n}","settings":{}},"ms.brands":{"_id":"54251f535978100b65dfc96b","active":"1","name":"ms.brands","type":"store","controller":"mystore.controllers.allBrands","settings":[{"id":12726,"name":"ms-data-fields","data_type":"text","settings":{"display_type":"text","validation":[],"required":"1","placeholder":"Array of fields","display_name":"Fields"},"original_name":"ms-data-fields","type":"text","label":"Fields"}]},"brand":{"name":"brand","type":"store","active":"1","_id":"542bf0b5a6f013d67549f98f","entities":[],"layout":"sidebar","meta_tags":[],"og_tags":[],"url":"/brand/:brand"},"search":{"_id":"545499c726bcd2dc026992d9","active":"1","name":"search","settings":{},"type":"store"},"ms.checkout.v1":{"_id":"5460678f2b4a67c122cde3e4","active":"0","name":"ms.checkout.v1","type":"store"},"ms.th1.checkout1":{"name":"ms.th1.checkout1","type":"store","active":"0","_id":"5460b04b1ca51e450460fd29"},"ms.checkout1":{"name":"ms.checkout1","type":"store","active":"1","_id":"547ea40dc70f99c80e88a6e8","controller":"function ($scope, app, $location, $stateParams, $rootScope) {\n    var cart = $rootScope.ms.cart, shipping_form_valid = true, user = $rootScope.ms.user;\n\n   