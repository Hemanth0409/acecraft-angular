ame, .quantity .lp {\r\n    text-transform: capitalize;\r\n    width: 100px;\r\n    color: white !important;\r\n    background: #000;\r\n    margin: 0;\r\n    height: 35px;\r\n    vertical-align: middle;\r\n    position: relative;\r\n    left: 4px;\r\n    padding: 10px;\r\n    display: inline-block;\r\n}\r\n#product-options .option-name {\r\n\theight: 59px;\r\n\tline-height: 59px;\r\n\tpadding: 0;\r\n\ttext-align: center;\r\n}\r\n#product-options .option-values {\r\n    display: inline-block;\r\n    max-width: 90%;\r\n}\r\n\r\n#product-options .option-values select {\r\n    width: 100%;\r\n    max-width: 350px;\r\n    display: inline-block;\r\n}\r\n</style>\r\n","/templates/widgets/register.html":"<div ms-widget=\"ms.th2.register_controller\" class=\"register-form\" ms-data-formname=\"register\" ms-data-buttonname=\"register\" class=\"\">\n    <form name=\"enquiry\" ng-submit=\"submit()\" ms-success=\"ms.location.path('/account/profile')\">\n        <div class=\"row social-login register-social-links text-center ms-fs-11\">\n            <h2 class=\" text-center secondary ms-mb-m main-heading\">{{'Signup with ACECRAFT'|msTranslate}}</h2>\n            <!-- <a ng-repeat=\"data in ms.settings.ms.login_providers\" ng-if=\"data.name == 'facebook'\" class=\"btn facebook-login register-social-links\" ms-widget=\"ms.facebookLogin\">\n                <i class=\"fa fa-facebook fa-lg\">&nbsp;</i>\n                {{'Facebook'|msTranslate}}\n            </a>\n\n            <a ng-repeat=\"data in ms.settings.ms.login_providers\" ng-if=\"data.name == 'google'\" type=\"button\" class=\" btn google-login register-social-links\" ms-widget=\"ms.googleLogin\">\n                <i class=\"fa fa-google-plus fa-lg\">&nbsp;</i>\n                {{'Google'|msTranslate}}</a>\n            <p class=\"gray-light ms-mt-s ms-mb-x\">- {{'OR USING EMAIL'|msTranslate}} -</p> -->\n        </div>\n        <div ms-widget=\"form_layout\"></div>\n    </form>\n</div>\n<div class=\"text-center ms-mt-x ms-mb-m register-already-account\">\n    <span class=\"ms-fs-12\">{{'Already have an account'|msTranslate}}?\n        <a class=\"secondary\"  href=\"/user/login\">\n            {{'Login'|msTranslate}}!\n        </a>\n    </span>\n</div>\n","/templates/widgets/form_layout.html":"<div ng-repeat=\"field in formFields\">\n        <div ng-if=\"field.settings.display_type=='email'\" class=\"form-group ms-mb-0\">\n    \n            <input data-ng-disabled=\"field.settings.read_only == '1'\" type=\"text\" class=\"form-control\" id=\"{{field.name}}\" ng-model='fields[field.name]'\n                   placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='text'\" class=\"form-group ms-mb-0\">\n    \n            <input data-ng-disabled=\"field.settings.read_only == '1'\" type=\"text\" class=\"form-control\" id=\"{{field.name}}\" ng-model='fields[field.name]'\n                   placeholder=\"{{field.settings.placeholder|msTranslate}}\" autocomplete=\"off\">\n        </div>\n    \n        <div class=\"form-group ms-mb-xs\" data-ng-if=\"field.settings.display_type=='phone'\">\n            <input type=\"tel\" id=\"phone\" class=\"form-control\" data-ng-model=\"fields[field.name]\"\n                   data-ng-intl-tel-input placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='password'\" class=\"form-group ms-mb-0\">\n    \n            <input type=\"password\" class=\"form-control\" id=\"{{field.name}}\" placeholder=\"{{field.settings.placeholder|msTranslate}}\"\n                   ng-model='fields[field.name]'>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='checkbox'\" class=\"checkbox\">\n            <input type=\"checkbox\" ng-model='fields[field.name]'>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='file'\" class=\"form-group ms-mb-0\" ms-widget=\"ms.instantFileUpload\"  \n             ms-data-verisioning=\"{{field.settings.disable_versioning}}\" ms-data-name=\"file_option\">\n    \n            <input id=\"{{uniqueId}}\" type=\"file\" ng-model=\"fields[field.name]\" data-name=\"file_option\"\n                   placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n    \n            <div ng-if=\"uploading\">\n                <img ng-src=\"{{'395.gif' | asset }}\" alt=\"loading...\"/>\n            </div>\n            <div ng-if=\"!uploading\">\n                <div ng-if=\"previewUrl\">\n                    <img ng-src=\"{{previewUrl}}\" style=\"height:100px;\"/>\n                </div>\n            </div>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='select'\" class=\"form-group ms-mb-0\">\n    \n            <select id=\"{{field.name}}\" class=\"form-control\" ng-model='fields[field.name]'\n                    ng-options=\"option.value as option.name for option in field.settings.options \">\n    \n            </select>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='widget'\" class=\"form-group ms-mb-xs\">\n            <div ms-widget=\"{{field.settings.widget}}\"></div>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='state'\" class=\"form-group ms-mb-xs\">\n            <div ms-widget=\"ms.statePicker\"   ms-data-country-field='fields.country'>\n                <div ng-if='states.length!=0' class=\"form-group\">\n                    <label for=\"country\"><b>{{field.label}}</b></label>\n                    <select name=\"state\" class=\"form-control\" id=\"state\" placeholder=\"state\"\n                            ng-model=\"fields.state\"\n                            ng-options=\"data.name as data.name for data in states\">\n                    </select>\n                    <span style=\"color:#F30B39\" class=\"error\"\n                          ng-show=\"errors.state\">{{errors.state}}</span>\n                </div>\n            </div>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='country'\" class=\"form-group ms-mb-xs\">\n            <div ms-widget=\"ms.countryPicker\"  >\n                <div ng-if=\"countries.length==1\">\n                    <div for=\"country\" ng-init=\"fields[field.name]=countries[0].code\">\n                                                      <span class=\"text-muted\">{{'Service available\n                                    only in'|msTranslate}} {{countries[0].country}}\n                                                      </span>\n                    </div>\n                </div>\n                <div ng-if=\"countries.length>1\">\n                    <label for=\"{{field.name}}\">{{field.label}}</label>\n                    <select name=\"country\" class=\"form-control\" id=\"{{field.name}}\"\n                            ng-model=\"fields[field.name]\"\n                            ng-options=\"data.code as data.country for data in countries\">\n                        <option value=''>Select {{field.name |msTranslate}}</option>\n                    </select>\n                </div>\n            </div>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='radio'\" class=\"form-group ms-mb-0 text-center\">\n    \n            <label class=\"radio-inline\" data-ng-repeat=\"option in field.settings.options\">\n                <input type=\"radio\" ng-model='fields[field.name]' name=\"inlineRadioOptions\" id=\"{{field.name}}\"\n                       ng-value=\"option.value\">\n                <b> {{option.name |msTranslate}} </b>\n            </label>\n        </div>\n    \n        <div ng-if=\"field.settings.display_type=='date'\" class=\"form-group ms-mb-0\">\n    \n            <input class=\"form-control\" ui-date-format=\"DD, d MM, yy\" ui-date ng-model='fields[field.name]'\n                   placeholder=\"{{field.settings.placeholder|msTranslate}}\">\n    \n        </div>\n    \n    \n        <div ng-if=\"field.settings.display_type=='textarea'\" class=\"form-group ms-mb-0\">\n    \n            <textarea id=\"{{field.name}}\" class=\"form-control\" ng-model='fields[field.name]' rows=\"3\"></textarea>\n        </div>\n    \n        <div class=\"form-group ms-mb-0\" ng-if=\"field.settings.display_type=='captcha'\">\n            <label for=\"{{field.name}}\">Captcha</label>\n            <div ms-widget=\"ms.form.captcha\"   ms-data-src=\"cdn\"></div>\n        </div>\n    \n        <label class=\"error\">{{errors[field.name]|msTranslate}}</label>\n    </div>\n    \n    \n    <div class=\"form-group ms-mb-0\">\n    \n        <div class=\"\">\n            <button type=\"submit\" class=\" register_1 ms-w-full btn \" style=\"background:black;color:white !important\">{{button|ms_ucfirst|msTranslate}}</button>\n        </div>\n    </div>\n\n    <style>\n        btn-default:hover {\n            background-color:black !important;\n        }\n        .register_1:hover, .register_1:active{\n            background:black;color:white !important\n\n        }\n        </style>","/templates/widgets/options.html":"<style>\n    #product-options .box.active {\n        border: 1px solid rgb(87, 167, 179);\n    }\n\n    .ms-d-tablecell {\n        display: table-cell\n    }\n\n    .ms-d-tr {\n        display: table-row\n    }\n    .option_val{min-width: 30px;}\n    #product-options .box {\n        width: auto !important;\n        min-width: 30px;\n        /*max-width: 30px;*/\n        background-size: 30px 30px;\n        padding: 0 4px !important;\n        border: 1px solid rgba(20, 19, 19, 0.24);\n        display: table;\n        overflow: hidden;\n        text-align: center;\n        background-repeat: no-repeat;\n        cursor: pointer;\n        color: black;\n        max-height: 30px !important;\n        min-height: 30px !important;\n    }\n\n    #product-options .box span {\n        display: table-cell;\n        vertical-align: middle;\n    }\n\n    #product-options .disable {\n        opacity: .5;\n    }\n\n    #product-options .option-name {\n        text-transform: capitalize;\n        width: 100px;;\n    }\n\n    #product-options .option-values {\n        display: inline-block;\n        max-width: 90%;\n    }\n\n    #product-options .option-values select {\n        width: 100%;\n        max-width: 350px;\n        display: inline-block;\n\n    }\n\n    #product-options .option-values .value-label {\n        font-size: 11px;\n        word-wrap: break-word;\n        text-align: center;\n    }\n\n    #product-options .option-values ul.nav li {\n        margin-left: 20px;\n    }\n\n    #product-options .option-values ul.nav li:first-child {\n        margin-left: 0px !important;\n    }\n\n    #product-options .radio {\n        display: inline-block !important;\n        margin-right: 10px !important;\n        margin-top: 0px !important;\n        margin-bottom: 5px !important;\n    }\n\n    #product-options .option-name .image, #product-options .option-name .swatch {\n        line-height: 34px;\n    }\n</style>\n\n<div id='product-options'>\n    <div ng-repeat=\"(key,value) in show_always_options\">\n\n        <div ng-if=\"value.type == 'date'\" class=\"option-values\">\n            <div class=\"form-group\">\n                <h5 class=\"option-name\"><b>{{value.display_name|msTranslate}}</b></h5>\n                <div class=\"row\">\n                    <div class=\"col-sm-12 option-name ms-pt-xs\"\n                         ng-init=\"dateOptions={changeYear:true,yearRange: '-50:+01',changeMonth: true,minDate:0}\">\n                        <input type=\"text\" ui-date=\"dateOptions\" name=\"date\"\n                               ng-model=\"product.selected_options[key]\"/>\n                    </div>\n                </div>\n                <p ng-if=\"value.show\" class=\"error\">{{value.required_message}}</p>\n            </div>\n        </div>\n\n        <div ng-if=\"value.type == 'textarea'\" class=\"option-values\">\n            <div class=\"form-group\">\n                <h5 class=\"option-name\"><b>{{value.display_name|msTranslate}}</b></h5>\n\n                <div class=\"row\">\n                    <div class=\" col-sm-12 option-name ms-pt-xs\">\n                            <textarea rows=\"3\" name=\"textarea\" cols=\"30\"\n                                      ng-model=\"product.selected_options[key]\"></textarea>\n                    </div>\n                </div>\n            </div>\n            <p ng-if=\"value.show\" class=\"error\">{{value.required_message|msTranslate}}</p>\n        </div>\n\n        <div ng-if=\"value.type =='dropdown'\">\n            <div class=\"form-group\">\n                <h5 class=\"option-name\"><b>{{value.display_name}}</b></h5>\n                <div class=\"option-values\">\n                    <div class=\"row\">\n                        <div class=\"col-sm-12\">\n                            <select ng-model=\"product.selected_options[key]\" class=\"form-control\"\n                                    ng-options=\"option.value as option.label for option in  show_always_options_values[key]\">\n                                <option value=\"\">{{'Please Select'|msTranslate}}</option>\n                            </select>\n                        </div>\n                    </div>\n                </div>\n                <p ng-if=\"value.show\" class=\"error\">{{value.required_message|msTranslate}}</p>\n            </div>\n        </div>\n\n        <div ng-if=\"value.type == 'image'\" class=\"option-values\">\n            <div class=\"col-sm-2 option-name\">\n                <strong class=\"image\"><span>{{value.display_name|msTranslate}}:</span></strong>\n            </div>\n            <div class=\"col-sm-10 option-values\">\n                <ul class=\"nav nav-pills\">\n                    <li ng-repeat=\"option in show_always_options_values[key]\">\n                        <a ng-class=\"{'active': option.value == product.selected_options[key]}\"\n                           class=\"box\" style=\"background-image: url({{option.image|image}});\"></a>\n\n                        <p class=\"value-label\">{{option.label|msTranslate}}</p>\n                    </li>\n                </ul>\n                <p ng-if=\"value.show\">{{value.required_message|msTranslate}}</p>\n            </div>\n        </div>\n\n    </div>\n\n    <div ng-if=\"product.options.length\">\n        <div ng-repeat=\"(key,value) in options\" class=\"ms-mb-s\">\n            <div ng-if=\"value.type == 'text-field'||value.type == 'dropdown'\">\n                <div class=\"form-group\">\n                    <label class=\"option-name\" style=\"position:relative;top:0px !important\">\n                        <span style=\"color:white !important\">{{value.display_name|msTranslate}}:</span>\n                    </label>\n\n                    <div class=\"option-values\">\n                        <select ng-model=\"product.selected_options[value.name]\" class=\"form-control \"\n                                ng-change=\"findRules(value.name,product.selected_options[value.name])\"\n                                ng-options=\"option.value as option.label disable when !option.available for option in  option_values[value.name]\">\n                            <option value=\"\" disabled>{{'Please Select'|msTranslate}}</option>\n                        </select>\n                    </div>\n                    <p ng-if=\"value.show\" class=\"error\">{{value.required_message|msTranslate}}</p>\n                </div>\n            </div>\n            <div class=\"row\" ng-if=\"value.type == 'radio'\">\n                <div class=\" col-sm-12 ms-pt-xs\">\n                    <label class=\"option-name\"> <strong>{{value.display_name|msTranslate}}:</strong></label>\n                    <label class=\"radio \"\n                           ng-repeat=\"option in option_values[value.name]\">\n                        <input ng-model=\"product.selected_options[value.name]\"\n                               ng-change=\"findRules(value.name,product.selected_options[value.name])\" type=\"radio\"\n                               class=\"form-controll\"\n                               id=\"optionsRadios1\" ng-disabled=\"!option.available\"\n                               value=\"{{option.value}}\">\n                        {{option.label}}\n                    </label>\n\n                    <p ng-if=\"value.show\" style=\"color:#980002\">{{value.required_message|msTranslate}}</p>\n\n                </div>\n            </div>\n            <div ng-if=\"value.type == 'swatch' && !((value.display_name == 'color') || (value.display_name == 'Color'))\">\n                <div class=\"option-values ms-mt-xs \">\n                    <div class=\"ms-d-table\">\n                        <div class=\"ms-d-tr\">\n                        <span class=\"ms-d-tablecell pull-left ms-w-100 swatch option-name ms-lh-30\">\n                            {{value.display_name|msTranslate}}:\n                        </span>\n                            <span class=\"ms-d-tablecell\">\n                     <span class=\"option_val ms-ml-xs ms-dis-ib\" ng-class='{\"disable\":!option.available}'\n                           ng-repeat=\"option in option_values[value.name]\">\n                        <a ng-class=\"{'active': option.value == product.selected_options[value.name]}\"\n                           class=\"box ms-fs-12\" data-toggle=\"tooltip\" title=\"{{option.value}}\"\n                           ng-click=\"findRules(value.name, option.value)\">\n                            <span>{{option.value|msTranslate}}</span>\n                        </a>\n                    </span>\n                      </span>\n                        </div>\n                    </div>\n                    <p ng-if=\"value.show\" style=\"color:#980002\">{{value.required_message|msTranslate}}</p>\n                </div>\n            </div>\n            <div ng-if=\"(value.type == 'swatch') && ((value.display_name == 'color') || (value.display_name == 'Color'))\">\n\n                <div class=\"option-values ms-mt-xs\">\n                    <div class=\"ms-d-table\">\n                        <div class=\"ms-d-tr\">\n                        <span class=\"ms-d-tablecell pull-left ms-w-100 swatch option-name ms-lh-30\">\n                            {{value.display_name|msTranslate}}:\n                        </span>\n                            <span class=\"ms-d-tablecell\">\n                     <span class=\"option_val ms-ml-xs ms-dis-ib\" ng-class='{\"disable\":!option.available}'\n                           ng-repeat=\"option in option_values[value.name]\">\n                        <a ng-class=\"{'active': option.value == product.selected_options[value.name]}\"\n                           class=\"box ms-fs-12\" data-toggle=\"tooltip\" title=\"{{option.value}}\"\n                           ng-style=\"{'background':'{{option.value}}'}\"\n                           ng-click=\"findRules(value.name, option.value)\"></a>\n                    </span>\n                      </span>\n                        </div>\n                    </div>\n                    <p ng-if=\"value.show\" style=\"color:#980002\">{{value.required_message|msTranslate}}</p>\n                </div>\n\n\n            </div>\n            <div class=\"row\" ng-if=\"value.type == 'image'\">\n                <div class=\"col-sm-2 option-name\">\n                    <strong class=\"image\"><span>{{value.display_name|msTranslate}}:</span></strong>\n                </div>\n                <div class=\"col-sm-10 option-values\">\n                    <ul class=\"nav nav-pills\">\n                        <li ng-class='{\"disable\":!option.available}'\n                            ng-repeat=\"option in option_values[value.name]\">\n                            <a ng-class=\"{'active': option.value == product.selected_options[value.name]}\"\n                               class=\"box\" style=\"background-image: url({{option.image|image}});\"\n                               ng-click=\"findRules(value.name, option.value)\"></a>\n\n                            <p class=\"value-label\">{{option.label|msTranslate}}</p>\n                        </li>\n                    </ul>\n                    <p ng-if=\"value.show\">{{value.required_message|msTranslate}}</p>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n","/templates/widgets/blog.html":"<div class=\"\">\n    <div class=\"wrapper\">\n        <!-- ol class=\"breadcrumb ms-pl-0\">\n            <li><a data-ng-href=\"/\">{{'Home'|msTranslate}}</a></li>\n            <li class=\"active\">{{'Blog'|msTranslate}}</li>\n\n        </ol -->\n\n        <div class=\"row blog-page\">\n\n            <div class=\"container ms-pl-md ms-pr-md\">\n                <!--Blog Post-->\n                <div ms-widget=\"ms.entity\" ms-data-filter='[{\"field\":\"publish\",\"value\":\"1\"}]'\n                     ms-data-entity=\"ms.blog_posts\" ms-data-loadMore=\"onScroll\" ms-data-bottomelementid=\"footer\"\n                     ms-name=\"blogs\">\n                    <h1 class=\"text-center proximaBold ms-mb-m ms-mt-x text-black\">{{'Blogs'|msTranslate}}</h1>\n\n                    <div ng-if='records.length==0'><h2>{{'No Blogs Found.'|msTranslate}}</h2></div>\n                    <div class=\"col-sm-12 col-md-6 ms-p-m\" ng-repeat=\"record in records\">\n                        <div class=\"funny-boxes\">\n\n                            <a class=\"thumbnail\" href=\"blog/{{record.alias}}\">\n<div class=\"ms-img-warpper\">\n<div class=\"img-place-holder\"><table><tbody><tr><td style=\"color:white !important\"> {{record.title|uppercase}} </td></tr></tbody></table></div>\n<div style=\"padding-top: 50%;\"></div>\n</div>\n                                <!-- img ms-widget='ms.responsiveImage' ms-data-aspect-ratio='2:1'\n                                     ms-data-valign=\"middle\" ms-halign=\"center\"\n                                     ms-data-placeholder-html='<div class=\"img-place-holder\"><span style=\"position:absolute; left:0; top:40%;  width:100%; font-size:18px; font-weight:600; color:#bbbbbb; text-align:center;background: black;\" > {{record.title|uppercase}}</span></div>' \n                                     ng-src=\"{{record.image | image:'o'}}\" -->\n                            </a>\n                            <!--<p ms-widget=\"ms.entity\" ms-data-entity=\"ms.blogs\" ms-data-filters='[{\"field\":\"_id\",\"value\":\"{{record._id}}\"}]'>\n                              <a ng-repeat=\"record in records\">{{record.title}}</a>\n                            </p>\n                            <a href=\"blog/{{record.alias}}\"><h4 class=\"blog-title ms-pl-m ms-pr-m \">\n                                {{record.title|uppercase}}</h4></a>\n                            <hr>\n                            <ul class=\"list-unstyled list-inline blog-info ms-pl-m ms-pr-m \">\n                                <li ng-if=record.author>{{'by'|msTranslate|ms_ucfirst}} {{record.author}}</li>\n                                <li ng-if=\"record.time\"><a ng-href=\"/blog/{{record.alias}}\" style=\"padding: 4px;\"><i\n                                        class=\"fa fa-clock-o\"></i>&nbsp;{{record.time | date:'MMM dd yyyy'}}</a></li>\n\n                                <li ng-if=\"record.num_comments> 1\" class=\"pull-right\"><a\n                                        ng-href=\"/blog/{{record.alias}}\" style=\"padding: 4px;\"><span\n                                > <i class=\"fa fa-comments\"></i>&nbsp; {{record.num_comments}} {{'comments'|msTranslate}}</span></a>\n                                </li>\n                                <li ng-if=\"record.num_comments== 1\" class=\"pull-right\"><a\n                                        ng-href=\"/blog/{{record.alias}}\" style=\"padding: 4px;\"><span\n                                > <i class=\"fa fa-comments\"></i>&nbsp;{{record.num_comments}} {{'comment'|msTranslate}}</span></a>\n                                </li>\n                                <li><i class=\"fa fa-comments\"></i> <a href=\"#\">24 Comments</a></li>\n                                <li ng-if=\"tag\"><i class=\"fa fa-tags\"></i>\n                                    <small ng-repeat=\"tag in record.tags\">{{tag}}<span\n                                            ng-if=\"record.tags.length > 1\">, </span></small>\n                                </li>\n                                <li class=\"pull-right\">\n                                    <p><a class=\"btn-sm btn-primary\" href=\"/blog/{{record.alias}}\"\n                                          style=\"padding: 4px;\"><i class=\"fa fa-location-arrow\"></i>{{'Read\n                                        More'|msTranslate}}</a></p>\n                                </li>\n                            </ul>-->\n                            <br>\n                        </div>\n\n\n                    </div>\n                    <!--End Blog Post-->\n                </div>\n\n                <!-- End Left Sidebar -->\n\n                <!-- Right Sidebar -->\n\n                <!-- End Right Sidebar -->\n            </div>\n\n            <!-- div class=\"col-md-3\">\n                <div ms-widget=\"ms.entity\" ms-data-entity=\"ms.blogs\">\n\n                    <h3 class=\"ms-mt-0\">{{'Blogs'|msTranslate}}</h3>\n\n                    <div ng-if='records.length==0'><h4>{{'No Blogs Found.'|msTranslate}}</h4></div>\n                    <div class=\"\" ng-repeat=\"record in records\">\n                        <div class=\"row\">\n                            <div class=\"col-sm-12\">\n                                <h4><a href=\"/blogs/{{record.alias}}\">{{record.title|ms_ucfirst}}</a></h4>\n                                <a href=\"/blogs/{{record.alias}}\"><p\n                                        ng-bind-html-unsafe=\"record.content|truncate:35\"></p></a>\n                            </div>\n                            <hr>\n                        </div>\n                    </div>\n                    <br>\n                </div>\n\n                <!--End category Blog Post-->\n                <!-- Start popular blog post-->\n                <!-- div ms-widget=\"ms.entity\" ms-data-entity=\"ms.blog_posts\" ms-data-pagesize=\"250\"\n                     ms-data-filters='[{\"field\":\"publish\",\"value\":\"1\"}]'>\n\n                    <h3 class=\"ms-mt-0\">\n                        {{'Popular Posts'|msTranslate}}</h3>\n\n                    <div ng-repeat=\"record in records| orderBy:'num_comments'\">\n\n                        <div class=\"row\">\n                            <div class=\"col-sm-12\">\n                                <h4><a href=\"/blog/{{record.alias}}\">{{record.title|ms_ucfirst}}</a></h4>\n\n                            </div>\n                            <hr>\n                        </div>\n                    </div>\n                </div>\n            </div -->\n        </div>\n    </div>\n</div>\n<style>\n\n    .thumbnail:hover img {\n        transition: all 1.3s 0.05s;\n        transform: rotate(0deg) scale(1);\n        -webkit-transform: scale(1.05) rotate(0);\n        transform: scale(1.05) rotate(0);\n    }\n\n    .thumbnail {\n        border: 0px ! important;\n    }\n\n    .funny-boxes {\n        margin-bottom: 25px;\n        box-shadow: 2px 2px 7px 0px rgb(186, 186, 186);\n    }\n.text-black {\ncolor: black;\n}\n.img-place-holder table{\nbackground: black;\n}\n.ms-img-warpper {\nbackground: black;\n}\n</style>","/templates/widgets/blog_detail.html":"<div class=\"bg-grey\" ms-widget=\"ms.blog_post\">\n    <div class=\"wrapper\">\n        <!-- ol class=\"breadcrumb ms-pl-0\">\n            <li><a data-ng-href=\"/\">{{'Home'|msTranslate}}</a></li>\n            <li><a data-ng-href=\"/blog\">{{'Blogs'|msTranslate}}</a></li>\n            <li ng-if=\"detail._id\" ms-widget=\"ms.entity\" ms-data-entity=\"ms.blogs\"\n                ms-data-filters='[{\"field\":\"_id\",\"value\":\"{{detail.blog}}\"}]'><a ng-href=\"/blogs/{{records[0].alias}}\">{{records[0].title}}</a>\n            </li>\n            <li class=\"active\">{{detail.title}}</li>\n        </ol -->\n\n        <div class=\"container\">\n            <!-- Left Sidebar -->\n            <div class=\"col-md-12\">\n                <!--Blog Post-->\n                <div class=\"blog ms-mt-x\">\n<h3 class=\"text-center ms-mt-x\"><span>{{detail.time | date:'MMM dd, yyyy'}}</span></h3>\n                    <p ng-if=\"loading || !detail\" class=\"text-center\"><img ng-src=\"{{'loading.gif' |asset}}\">\n                    </p>\n\n                    <h1 class=\"ms-mt-m text-center\">{{detail.title|lowercase|ms_ucfirst}}</h1>\n\n                    <!-- div class=\"ms-mt-m  ms-mb-m\">\n                        <ul class=\"list-unstyled list-inline ms-pl-0 ms-pb-s {{detail.metafields.color_code}}\">\n                            <li ng-if=\"detail.author\">{{'By'|msTranslate}} {{detail.author}}</li>\n                            <li ng-if=\"detail.time\"><span ng-if=\"detail.author\">&bull;&nbsp;</span><span>{{detail.time | date:'MMM dd, yyyy'}}</span>\n                            </li>\n                            <li ng-if=\"detail.num_comments> 1\"><a ms-scroll=\"comments\" style=\"padding: 4px;\"><span\n                                    ng-if=\"detail.author && detail.time\">&bull;&nbsp;&nbsp;</span><span\n                            > {{detail.num_comments}} {{'comments'|msTranslate}}</span></a>\n                            </li>\n                            <li ng-if=\"detail.num_comments== 1\"><a ms-scroll=\"comments\" style=\"padding: 4px;\"><span\n                                    ng-if=\"detail.author && detail.time\">&bull;&nbsp;&nbsp;</span><span\n                            > {{detail.num_comments}} {{'comment'|msTranslate}}</span></a>\n                            </li>\n                            <li class=\"pull-right\">\n                                <div ms-widget=\"ms.addthis\" class=\"addthis_toolbox addthis_default_style \">\n                                    <a class=\"addthis_button_facebook addthis_button_preferred_1\"></a>\n                                    <a class=\"addthis_button_gmail addthis_button_preferred_2\"></a>\n                                    <a class=\"addthis_button_google_plusone_share addthis_button_preferred_3\"></a>\n                                    <a class=\"addthis_button_pinterest_share addthis_button_preferred_4\"></a>\n                                    <a class=\"addthis_button_compact\"></a>\n                                    <a class=\"addthis_counter addthis_bubble_style\"></a>\n                                </div>\n                            </li>\n                        </ul>\n                        <ul ng-if=\"detail.tags.length\" class=\"list-unstyled list-inline blog-tags\">\n                            <li style=\"margin: 0px;\"><i class=\"fa fa-tags\"></i></li>\n                            <li ng-repeat=\"tag in detail.tags\">\n                                <a href=\"/blogs/tag/{{tag}}\">{{tag|ms_ucfirst}}</a>\n                            </li>\n                        </ul>\n                    </div -->\n                    <!-- div class=\"blog-img ms-mb-l thumbnail\">\n\n                        <img class=\"\" ms-widget='ms.responsiveImage' ms-data-aspect-ratio='2:1'\n                             ms-data-valign=\"middle\" ms-data-halign=\"center\"\n                             ms-data-placeholder-html='<div class=\"img-place-holder\" ><span style=\"position:absolute; left:0; top:40%;  width:100%; font-size:18px; font-weight:600; color:#bbbbbb; text-align:center;\" >Blog Image</span></div>'\n                             ng-src=\"{{detail.image | image:'o'}}\" style=\"width: 100%;\">\n                    </div-->\n\n                    <p ng-bind-html-unsafe=\"detail.content\" class=\"ms-fs-14\" style=\"word-wrap: break-word; max-width: 800px; margin: auto;\"></p>\n                </div>\n                <!--End Blog Post-->\n\n                <hr>\n                <!-- Recent Comments -->\n                <div ng-if=\"detail._id\">\n                    <div id=\"comments\" class=\"media\" ms-widget=\"ms.getComment\" ms-data-unique_id='{{detail._id}}'\n                         ms-name=\"getComment\">\n                        <h3>{{comments.length}} <span ng-if=\"comments.length>1\">{{'Comments'|msTranslate}}</span><span\n                                ng-if=\"comments.length<=1\">{{'Comment'|msTranslate}}</span></h3>\n\n                        <div ng-repeat=\"comment in comments\">\n\n\n                            <!---------------------------------->\n                            <blockquote>\n                                <p ng-bind-html-unsafe=\"comment.comment\"></p>\n\n                          <span> <footer>{{'By:'|msTranslate}} <cite title=\"Source Title\"> {{comment.user_name}}</cite>\n                          </footer>\n                            <small>\n                                {{comment.created_on | date:'MMM dd, yyyy'}}&nbsp;&nbsp;\n                                <a data-toggle=\"collapse\" href=\"#{{$index}}\" class=\" ms-mt-xs \" style=\"color: #999999;\">{{'Reply'|msTranslate}}</a>\n                            </small></span>\n\n\n                                <div ng-if=\"comment.reply.length> 0\">\n                                    <hr>\n\n                                    <h5>{{'Replies :'|msTranslate}} </h5>\n\n                                    <div>\n                                        <div ng-repeat=\"reply in comment.reply\">\n\n                                            <em class=\" ms-ml-l \">{{reply.comment}}</em>\n                                            <small class=\"ms-mt-xs ms-ml-l\"> {{reply.user_name}} -\n                                                {{reply.created_on |\n                                                date:'longDate'}}, {{reply.created_on | date:'mediumTime'}}\n                                            </small>\n\n                                        </div>\n                                    </div>\n                                </div>\n                            </blockquote>\n\n\n                            <div id=\"{{$index}}\" class=\"collapse\">\n                                <!-- Comment Reply Form -->\n                                <div class=\"post-comment\">\n                                    <h3>{{'Leave a Reply'|msTranslate}}</h3>\n\n                                    <form class=\"form-horizontal\" ms-widget=\"ms.form.addBlogCommentReply\"\n                                          name=\"blogform1\"\n                                          ms-data-commentid=\"{{comment._id}}\"\n                                          ms-success=\"ms.widgets.getComment.loadCommentsReply()\">\n\n                                        <div class=\"error\" style=\"color:#ff0000;\" ng-if=\"type=='error'\"\n                                             ng-show=\"message\">{{message}}\n                                        </div>\n                                        <div style=\"color:green;\" ng-if=\"type=='success'\" id=\"success\"\n                                             ng-show=\"message\">\n                                            {{message}}\n                                        </div>\n                                        <br>\n                                        <label>{{'Name'|msTranslate}}</label>\n\n                                        <div class=\"row ms-mb-l\">\n                                            <div class=\"col-md-7 col-md-offset-0\">\n                                                <input type=\"text\" name=\"username\" class=\"form-control\"\n                                                       id=\"nameReply\"\n                                                       placeholder=\"Name\" ng-model=\"user_name_reply\"/>\n                                            </div>\n                                        </div>\n                                        <label>{{'Email'|msTranslate}}</label>\n\n                                        <div class=\"row ms-mb-l\">\n                                            <div class=\"col-md-7 col-md-offset-0\">\n                                                <input class=\"form-control\" type=\"email\" name=\"email1\"\n                                                       id=\"inputEmail\"\n                                                       placeholder=\"Email\" ng-model=\"user_email_reply\"/>\n                                                   <span class=\"error\" ng-show=\"blogform1.email1.$error.email\">\n        Invalid Email</span>\n                                            </div>\n                                        </div>\n                                        <label>{{'Message'|msTranslate}}</label>\n\n                                        <div class=\"row ms-mb-l\">\n                                            <div class=\"col-md-7 col-md-offset-0\">\n                                                <textarea name=\"password\" class=\"form-control\" rows=\"3\"\n                                                          id=\"commentReply\"\n                                                          placeholder=\"Your Comment\"\n                                                          ng-model=\"comment_reply\"></textarea>\n                                            </div>\n                                        </div>\n                                        <p class=\"ms-mt-m\">\n                                            <button class=\"btn btn-primary\" type=\"submit\"\n                                                    ng-click=\"submit(detail)\">{{'Send Message'|msTranslate}}\n                                            </button>\n                                        </p>\n                                    </form>\n                                </div>\n                                <hr>\n                                <!-- End Comment Reply Form -->\n                            </div>\n                        </div>\n                    </div>\n                    <!--/media-->\n                </div>\n                <!-- End Recent Comments -->\n\n                <hr style=\"margin: 30px 0;\">\n                <!-- Comment Form -->\n                <div class=\"post-comment\">\n                    <h3>{{'Leave a Comment'|msTranslate}}</h3>\n\n                    <div>\n                        <form class=\"form-horizontal\" ms-widget=\"ms.form.addBlogComment\" name=\"blogform2\"\n                              ms-success=\"ms.widgets.getComment.loadComments()\">\n\n                            <div class=\"error\" ng-if=\"type=='error'\" ng-show=\"message\">{{message}}</div>\n                            <div style=\"color:green;\" ng-if=\"type=='success'\" id=\"success\" ng-show=\"message\">{{message}}\n                            </div>\n                            <br>\n                            <label>{{'Name'|msTranslate}}</label>\n\n                            <div class=\"row ms-mb-m \">\n                                <div class=\"col-md-7 col-md-offset-0\">\n                                    <input type=\"text\" name=\"username\" class=\"form-control\" id=\"name\"\n                                           placeholder=\"Name\"\n                                           ng-model=\"user_name\"/>\n                                </div>\n                            </div>\n\n                            <label>{{'Email'|msTranslate}}</label>\n\n                            <div class=\"row ms-mb-m \">\n                                <div class=\"col-md-7 col-md-offset-0\">\n                                    <input class=\"form-control\" type=\"email\" name=\"email2\" id=\"inputEmail5\"\n                                           placeholder=\"Email\" ng-model=\"user_email\"/>\n                                    <span class=\"error\" ng-show=\"blogform2.email2.$error.email\">\n       Invalid Email</span>\n                                </div>\n                            </div>\n\n                            <label>{{'Message'|msTranslate}}</label>\n\n                            <div class=\"row ms-mb-m \">\n                                <div class=\"col-md-7 col-md-offset-0\">\n                                    <textarea name=\"password\" class=\"form-control\" rows=\"3\" id=\"comment\"\n                                              placeholder=\"Your Comment\" ng-model=\"comment\"></textarea>\n                                </div>\n                            </div>\n                            <p class=\"ms-mt-m\">\n                                <button class=\"btn btn-primary\" type=\"submit\" ng-click=\"submit(detail)\">{{'Post\n                                    Comment'|msTranslate}}\n                                </button>\n                            </p>\n\n                        </form>\n                    </div>\n                </div>\n                <!-- End Comment Form -->\n            </div>\n          \n        </div>\n        <!--/row-->\n    </div>\n</div>\n<style>\n.blog * {\ncolor: black;\n}\n</style>","/templates/widgets/staticpage.html":"<!--Don't delete this, it is widget in all stores access directly from themes-->\n<div id=\"columns\" class=\"staticpage\" ms-widget=\"ms.staticPage\">\n    <div ng-if=\"loading\">\n        <div class=\"load-bar\">\n            <div class=\"bar\"></div>\n            <div class=\"bar\"></div>\n            <div class=\"bar\"></div>\n        </div>\n    </div>\n    <div class=\"container\" style=\"width: 100%;\">\n        <div class=\"wrapper\">\n\n            <div ng-if=\"!loading\" class=\"row\">\n                <div class=\"col-sm-12\">\n                    <ol class=\"breadcrumb hidden-ms hidden-xs {{page.title}}\">\n                        <li><a href=\"/\" class=\"gray-light\">{{'Home'|msTranslate}}</a></li>\n                        <li class=\"active\">{{page.title}}</li>\n                    </ol>\n                    <h1 class=\"static-page-heading {{page.title}} {{page.alias}}\">\n                        {{page.title}}\n                    </h1>\n\n                    <div class=\"static-page-description\">\n                        <div class=\"ms-mb-x static-page-content\" ng-bind-html-unsafe=\"page.content\"></div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n</div>","/templates/widgets/product_page.html":"<div class=\"product-page\" ms-widget=\"ms.product\">\n    <div ng-if=\"loading\" >\n        <div class=\"load-bar\">\n            <div class=\"bar\"></div>\n            <div class=\"bar\"></div>\n            <div class=\"bar\"></div>\n        </div>\n    </div>\n    <div class=\"container\">\n        <div class=\"wrapper\">\n            <div class=\"row\">\n                <div class=\"col-sm-12 col-xs-12\">\n                    <ol class=\"breadcrumb hidden-xs hidden-ms ms-pl-0 ms-mb-l ms-mt-s ms-bg-none\">\n                        <li>\n                            <a href=\"/\" class=\"gray-light\">\n                                {{'Home'|msTranslate}}\n                            </a>\n                        </li>\n\n                        <li ng-if=\"product.category.parent.name\">\n                            <a href=\"/browse/{{product.category.parent.alias}}\" class=\"gray-light\">\n                                {{product.category.parent.name}}\n                            </a>\n                        </li>\n                        <li ng-if=\"product.category\">\n                            <a data-ng-href=\"/browse/{{product.category.alias}}\" class=\"gray-light\">\n                                {{product.category.name}}\n                            </a>\n                        </li>\n\n                        <li class=\"active\">\n                            {{product.name }}\n                        </li>\n                    </ol>\n                    <div ms-widget=\"product\"></div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<style>\n.product-page {\nbackground-color: #fff;\npadding-top: 80px;\n}\n.product-page * {\ncolor: black;\n}\n.btn-primary, .gray-lighter {\ncolor: white !important;\n}\n</style>","/templates/widgets/category.html":"<div class=\"banner text-center\">\n<div class=\"proximaThin\">Decode</div>\n<div>your kind of designer uniforms.</div>\n</div>\n<div ng-if=\"ms.variables.pageview == 'pagination'\">\n    <div ng-if=\"!ms.location.search().page\" data-ng-init=\"ms.location.search('page',1)\"></div>\n    <div ms-widget=\"ms.categoryProducts\" ms-data-pagesize=\"{{ms.variables.category_page_size}}\" ms-data-total=\"1\"\n         ms-name=\"category_products\">\n        <div ng-if=\"loading\">\n            <div class=\"load-bar\">\n                <div class=\"bar\"></div>\n                <div class=\"bar\"></div>\n                <div class=\"bar\"></div>\n            </div>\n        </div>\n        <div ng-if=\"ms.settings.faceted_search == '1'\">\n            <div class=\"category-page\" ms-widget=\"ms.th3.getFacets\" ms-data-widget=\"category_products\">\n                <div class=\"container\">\n                    <div class=\"wrapper ms-pb-m ms-pt-s\">\n                        <div class=\"cat-breadcrumb\">\n                            <ol class=\"breadcrumb hidden-xs hidden-ms ms-pl-0 ms-mb-l ms-mt-s ms-bg-none\">\n                                <li><a href=\"/\" class=\"gray-light\"> {{'Home'|msTranslate}} </a></li>\n                                <li ng-if=\"category.parent.parent.parent.parent\"><a\n                                        data-ng-href=\"/browse/{{category.parent.parent.parent.parent.alias}}\"\n                                        class=\"gray-light\"> {{category.parent.parent.parent.parent.name}} </a></li>\n                                <li ng-if=\"category.parent.parent.parent\"><a\n                                        data-ng-href=\"/browse/{{category.parent.parent.parent.alias}}\"\n                                        class=\"gray-light\"> {{category.parent.parent.parent.name}} </a></li>\n                                <li ng-if=\"category.parent.parent\"><a\n                                        data-ng-href=\"/browse/{{category.parent.parent.alias}}\" class=\"gray-light\">\n                                    {{category.parent.parent.name}} </a></li>\n                                <li ng-if=\"category.parent\"><a data-ng-href=\"/browse/{{category.parent.alias}}\"\n                                                               class=\"gray-light\"> {{category.parent.name}} </a></li>\n                                <li class=\"active\"> {{category.name}}</li>\n                            </ol>\n                        </div>\n                        <div class=\"row\">\n                            <div ng-if=\"ms.variables.show_sidebar=='yes'\" class=\"col-sm-4 col-md-12\">\n                                <div ms-widget=\"sidebar\"></div>\n                            </div>\n                            <div class=\"col-md-12\">\n                                <div class=\"row category-page\" style=\"border-left: 1px solid #efefef;\">\n                                    <div class=\"col-sm-12 ms-pl-m\">\n                                        <div class=\"row ms-mt-m ms-mb-m ms-pb-s\"\n                                             style=\"border-bottom: 1px solid #f3f3f3;\">\n                                            <div class=\"col-sm-12\" ng-init=\"\"><h1 class=\"category-name\" id=\"top\">\n                                                {{category.name}} </h1>\n                                                <span class=\"horizontal-filters-sub ms-ml-m\" ng-if=\"!products.length\">\n                                                    {{'No items in here'|msTranslate}}\n                                                </span>\n                                                <span class=\"horizontal-filters-sub ms-ml-m\"\n                                                      ng-show=\"ms.location.search().page != 1 && (totalRecords > (ms.location.search().page-1)*(ms.variables.category_page_size)+(products.length))\">\n                                                    Showing {{((ms.location.search().page-1)*(ms.variables.category_page_size))*1+1}} to\n                                                    {{(ms.location.search().page-1)*(ms.variables.category_page_size)+(products.length)}} of\n                                                    {{totalRecords}} items\n                                                </span>\n                                                <span class=\"horizontal-filters-sub ms-ml-m\" ng-show=\"ms.location.search().page != 1 && (totalRecords <= (ms.location.search().page-1)*(ms.variables.category_page_size)+(products.length))\">\n                                                    Showing {{((ms.location.search().page-1)*(ms.variables.category_page_size))*1+1}} to\n                                                    {{totalRecords}} of\n                                                    {{totalRecords}} items\n                                                </span>\n                                                <span class=\"horizontal-filters-sub ms-ml-m\" ng-show=\"ms.location.search().page == 1 && products.length\">\n                                                    {{'Showing'|msTranslate}} 1 to {{products.length}} {{'out of'|msTranslate}}\n                                                    {{totalRecords}} {{'products'|msTranslate}}.\n                                                </span>\n                                                <div class=\"pull-right hidden-xs hidden-ms\" ng-if=\"products.length\"\n                                                     style=\"margin-top:-7px\">\n                                                    <div class=\"sort-sortBy\"> {{'Sort By'|msTranslate|truncate:10}}:\n                                                        <span>{{selected|msTranslate|truncate:10}}</span> <i\n                                                                class=\"fa fa-angle-down ms-lh-20 pull-right\"\n                                                                aria-hidden=\"true\"> </i>\n                                                        <ul class=\"sort-list list-unstyled\" data-ng-init=\"selected='' \">\n                                                            <li data-ng-click=\"col='price';sort(col);selected='Low to High'\"\n                                                                data-ng-class=\"{active:col=='price'}\"><label\n                                                                    class=\"sort-label \"> <input type=\"radio\"\n                                                                                                value=\"low\">\n                                                                {{'Price'|msTranslate}}: {{'Low to High'|msTranslate}}\n                                                            </label></li>\n                                                            <li data-ng-click=\"col='-price';sort(col);selected='High to Low'\"\n                                                                data-ng-class=\"{active:col=='-price'}\"><label\n                                                                    class=\"sort-label \"> <input type=\"radio\"\n                                                                                                value=\"high\">\n                                                                {{'Price'|msTranslate}}: {{'High to Low'|msTranslate}}\n                                                            </label></li>\n                                                            <li data-ng-click=\"col='alias';sort(col);selected='Ascending'\"\n                                                                data-ng-class=\"{active:col=='alias'}\"><label\n                                                                    class=\"sort-label \"> <input type=\"radio\"\n                                                                                                value=\"low\">\n                                                                {{'Name'|msTranslate}}: {{'Ascending\n                                                                Order'|msTranslate}} </label></li>\n                                                            <li data-ng-click=\"col='-alias';sort(col);selected='Descending'\"\n                                                                data-ng-class=\"{active:col=='-alias'}\"><label\n                                                                    class=\"sort-label \"> <input type=\"radio\"\n                                                                                                value=\"high\">\n                                                                {{'Name'|msTranslate}}: {{'Descending\n                                                                Order'|msTranslate}} </label></li>\n                                                        </ul>\n                                                    </div>\n                                                </div>\n                                            </div>\n                                            <div class=\"primary ms-mt-m hidden-xs hidden-ms\">\n                                                <div class=\"col-sm-12\">\n                                                    <ul class=\"list-inline\">\n                                                        <li ng-repeat=\"(key,record) in fields\" ng-if=\"record.length\"\n                                                            class=\"ms-mt-m ms-br-b ms-mr-xs ms-p-xs